<!--contact-->
 <%= form_with scope: :lead, url: lead_path, local: true do |form| %>
<section id="contact">
    <div class="container">

        <header class="text-center mb-60">
            <h2>Contact Us</h2>
            <p class="lead font-lato">Leave us a message or visit our office</p>
            <hr />
        </header>


        <div class="row">

            <!-- FORM -->
           
            <div class="col-md-8 col-sm-8">

                <h3>You can leave us any message by filling up <br/> this form.</h3>

                <!-- Alert Success -->
                <div id="alert_success" class="alert alert-success mb-30">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <strong>Thank You!</strong> Your message successfully sent!
                </div>
                <!-- /Alert Success -->


                <!-- Alert Failed -->
                <div id="alert_failed" class="alert alert-danger mb-30">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <strong>[SMTP] Error!</strong> Internal server error!
                </div>
                <!-- /Alert Failed -->

                <!-- Alert Mandatory -->
                <div id="alert_mandatory" class="alert alert-danger mb-30">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <strong>Sorry!</strong> You need to complete all mandatory (*) fields!
                </div>
                <!-- /Alert Mandatory -->

                
                <form enctype="multipart/form-data">
                    <fieldset>
                        <input type="hidden" name="action" value="contact_send" />

                        <div class="row">
                            <div class="col-md-4">
                                <label for="name">Full Name *</label>
                                <input required type="text" value="" class="form-control" name="name" id="name" style="font-size:100%">
                            </div>
                            <div class="col-md-8">
                                <label for="email">E-mail Address *</label>
                                <input required type="email" value="" class="form-control" name="email" id="email" style="font-size:100%">
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-4">
                                <label for="phone">Phone</label>
                                <input type="text" value="" class="form-control" name="phone" id="phone" style="font-size:100%">
                            </div>
                            <div class="col-md-8">
                                <label for="company">Company Name</label>
                                <input type="text" value="" class="form-control" name="company" id="company_name" style="font-size:100%">
                            </div>
                        </div>



                        <div class="row">


                            <div class="col-md-4">
                                <label for="project_name">Project Name *</label>
                                <input required type="text" value="" class="form-control" name="project_name" id="project_name" style="font-size:100%">
                            </div>
                            <div class="col-md-8">
                                <label for="project_description">Project Description </label>
                                <input type="text" value="" class="form-control" name="project_description" id="project_description" style="font-size:100%">

                            </div>
                        </div>

                        <div class="row">

                            <div class="col-md-4">
                                <label for="department">Department</label>
                                <select class="form-control pointer" name="department" id="department" style="font-size:100%">
                                                <option value="">--- Select ---</option>
                                                <option value="Marketing">Marketing</option>
                                                <option value="Engineering">Engineering</option>
                                                <option value="Architecture">Architecture</option>
                                </select>
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-md-12">
                                <label for="message">Message *</label>
                                <textarea required maxlength="10000" rows="8" style="font-size:100%" class="form-control" name="message" id="message"></textarea>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10">
                                <%# <label for="attachment">File Attachment</label>

                                <!-- custom file upload -->
                                <input class="custom-file-upload" type="file" id="file" name="attachment" id="attachment" data-btn-text="Select a File" />
                                <small class="text-muted block">Max file size: 10Mb (zip/pdf/jpg/png)</small> %>
                                <label for="message">Attachment *</label>
                                 <p>
                                     <%= form.file_field :attachment %>
                                 </p>

                            </div>
                        </div>


                    </fieldset>

                    <div class="row">
                        <div class="col-md-12">
                            <%= form.submit %>
                        </div>
                    </div>

                </form>

            </div>

            <!-- /FORM -->

            <!-- INFO -->
